FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

libs            ("liboverset.so");

application     overInterDyMFoam ;

startFrom       latestTime;

startTime       0.0;

stopAt          endTime;

endTime         36.0;

deltaT          0.001;

writeControl    adjustableRunTime;

writeInterval   0.12;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable yes;

adjustTimeStep  yes;

maxCo           0.3;

maxAlphaCo      0.3;

maxDeltaT       0.001;


functions
{
	GaugeVOF
    	{
        	type	sets;
			libs	("libsampling.so");

			interpolationScheme cell;

			setFormat       raw;

			surfaceFormat   raw;
	
			sets
			(    
				GaugeVOF00
				{
					type    midPointAndFace;
					axis    xyz;
					start   (  -15.0 0.45 -0.20);
					end     (  -15.0 0.45  0.20);
				}
				GaugeVOF01
				{
					type    midPointAndFace;
					axis    xyz;
					start   (  -5.0 0.45 -0.20);
					end     (  -5.0 0.45  0.20);
				}
				GaugeVOF02
				{
					type    midPointAndFace;
					axis    xyz;
					start   (  -1.0 0.45 -0.20);
					end     (  -1.0 0.45  0.20);
				}
				GaugeVOF03
				{
					type    midPointAndFace;
					axis    xyz;
					start   (  1.0 0.45 -0.20);
					end     (  1.0 0.45  0.20);
				}
				GaugeVOF04
				{
					type    midPointAndFace;
					axis    xyz;
					start   (  5.0 0.45 -0.20);
					end     (  5.0 0.45  0.20);
				}
				GaugeVOF05
				{
					type    midPointAndFace;
					axis    xyz;
					start   ( 10.0 0.45 -0.20);
					end     ( 10.0 0.45  0.20);
				}
				GaugeVOF06
				{
					type    midPointAndFace;
					axis    xyz;
					start   ( 12.0 0.45 -0.20);
					end     ( 12.0 0.45  0.20);
				}
			);
 
			fields          (alpha.water);
    	}

}


