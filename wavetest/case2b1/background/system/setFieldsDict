FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.water 0
    volScalarFieldValue zoneID 123
    
);

regions
(
    boxToCell
    {
        box (-100 -100 -100) (100 100 0.65);
        fieldValues ( volScalarFieldValue alpha.water 1 );
    }

    

   
    

);

